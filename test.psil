#!/opt/local/bin/python2.5 psil.py

(import 'sys)

((getattr (getattr sys "stdout") "write") "hello world\n") ; this is a comment
(.write (getattr sys "stdout") "hello world\n")
(display "hello world")

(import 'hashlib)
(define h (.sha1 hashlib))
(.update h "Hello world")
(display (.hexdigest h))

(define d (dict '(("a" "b") ("c" "d"))))
(display d)
(display (len d))
(display (.items d))
