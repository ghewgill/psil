>>> from psil import psil

>>> psil("(abs -5)")
5
>>> psil("(range 5)")
[0, 1, 2, 3, 4]
>>> psil("(map (lambda (x) (* x x)) '(1 2 3))")
[1, 4, 9]
>>> psil("(map (lambda (x y) (* x y)) '(1 2 3) '(4 5 6))")
[4, 10, 18]
>>> psil("(import 'os)") #doctest: +ELLIPSIS
<module 'os' ...>
>>> psil("(getattr os \"name\")")
'posix'

>>> psil("""(define f (open "integ.test"))""")
<open file 'integ.test', mode 'r' at 0x...>
>>> psil("(.readline f)")
'>>> from psil import psil\n'
>>> psil("(.close f)")

>>> psil("(import 'hashlib)")
<module 'hashlib' from '...'>
>>> psil("(define h (.sha1 hashlib))")
<sha1 HASH object @ 0x...>
